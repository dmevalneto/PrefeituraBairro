@model IEnumerable<IssatisLab.Yima.Mvc.Seed.Models.OcorrenciaInterna>

@{

    ViewBag.Title = "Relação de Ocorrência";
}


<p>
    <h3>  @Html.ActionLink("Nova Ocorrência", "Create") </h3>
</p>

<hr/>

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-white">
            <div class="panel-heading">
                <h3 class="panel-title">Relação de Ocorrência Interna</h3>
                <div class="panel-tools">
                    <a class="tools-action" href="#" data-toggle="collapse">
                        <i class="pe-7s-angle-up"></i>
                    </a>
                    <a class="tools-action" href="#" data-toggle="dispose">
                        <i class="pe-7s-close"></i>
                    </a>
                </div>
            </div>
            <div class="panel-body">
                <table class="table table-striped table-bordered table-hover">
                    <thead>
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.ocorrenciaInternaId)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.logradouro)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.bairro)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.status)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.dataIniciado)
                            </th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.ocorrenciaInternaId)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.logradouro)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.bairro)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.status)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.dataIniciado)
                            </td>
                            <td>
                                @Html.ActionLink("Editar", "Edit", new { id = item.ocorrenciaInternaId }) |
                                @Html.ActionLink("Detalhes", "Details", new { id = item.ocorrenciaInternaId }) |
                                @Html.ActionLink("Deletar", "Delete", new { id = item.ocorrenciaInternaId }) |
                                @Html.ActionLink("Tarefa", "Delete", new { id = item.ocorrenciaInternaId }) |
                                @Html.ActionLink("Add", "Delete", new { id = item.ocorrenciaInternaId })
                            </td>
                        </tr>
                        }
                        </tbody>
                </table>
            </div>
        </div>
    </div>
</div>