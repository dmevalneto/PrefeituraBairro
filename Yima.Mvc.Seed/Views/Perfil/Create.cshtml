@model IssatisLab.Yima.Mvc.Seed.Models.Perfil

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>


@using (var f = Html.BootstrapBeginForm().Horizontal(BootstrapGridSize.Medium2, BootstrapGridSize.Medium10).Begin()) 
{
    @Html.AntiForgeryToken()
    
    <h4>Perfil</h4>
    <hr />
        @f.FormGroup().TextBoxFor(model => model.nome)
        @f.FormGroup().TextBoxFor(model => model.telefone)
        @f.FormGroup().TextBoxFor(model => model.contato)
        @f.FormGroup().Submit("Create")
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
